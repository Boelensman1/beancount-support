{
  "extends": "@tsconfig/node20/tsconfig.json",
  "compilerOptions": {
    "types": ["node"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "strictBindCallApply": true,
    "useDefineForClassFields": false,
    "esModuleInterop": true,
    "noEmit": true
  }
}
